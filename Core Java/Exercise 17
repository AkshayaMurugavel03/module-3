 class Car {
    String make, model;
    int year;
    void displayDetails() {
        System.out.println("Make: " + make + ", Model: " + model + ", Year: " + year);
    }
 }
 public class CarTest {
    public static void main(String[] args) {
        Car c = new Car();
        c.make = "Toyota";
        c.model = "Corolla";
        c.year = 2020;
        c.displayDetails();
    }
 }